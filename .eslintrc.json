{
    "env": {
        "node": true,
        "browser": true,
        "es6": true
    },
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/prop-types": ["error", { "ignore": ["navigation"] }],
        "global-require": 0,
        "prefer-destructuring": ["error", {
            "VariableDeclarator": {
                "array": false,
                "object": false

            },
            "AssignmentExpression": {
                "array": false,
                "object": false
            }
        }, {
            "enforceForRenamedProperties": false
        }]

    }
}